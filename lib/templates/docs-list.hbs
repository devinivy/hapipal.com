{{#> layout title="Docs List" }}

    {{#*inline "content"}}
        <div class="docs-list">
            <div class="wrapper">
                <ul>
                    {{#each packages}}
                        <li>
                            <a href="/docs/{{this.name}}">{{this.name}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    {{/inline}}

{{/layout}}
