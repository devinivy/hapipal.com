{{#> layout title="API Docs" }}

    {{#*inline "content"}}
        <div class="main layout-list docs-list">
            <div class="wrapper">
                <h1>API Docs</h1>
                {{#each packages}}
                    <a href="/docs/{{this.name}}">
                      <h2>{{this.name}}</h2>
                      <div class="img"></div>
                      <p>{{this.subtitle}}</p>
                    </a>
                {{/each}}
            </div>
        </div>
    {{/inline}}

{{/layout}}
